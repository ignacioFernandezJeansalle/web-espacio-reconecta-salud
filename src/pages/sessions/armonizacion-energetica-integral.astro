---
import Layout from "@/layouts/Layout.astro";
import GeneralHero from "@/components/GeneralHero.astro";
import SessionInfo from "@/components/armonizacion-energetica-integral/ArmonizacionEnergeticaIntegralSessionInfo.astro";
import Details from "@/components/armonizacion-energetica-integral/ArmonizacionEnergeticaIntegralDetails.astro";

const TITLE = "Armonización energética integral";
const DESCRIPTION =
  "¡Liberá tu energía y transformá tu vida! Si estás buscando eliminar los bloqueos energéticos que te tienen estancada, elevar la frecuencia vibracional de todas las áreas de tu vida y crear un cambio profundo, simple y con efectos rápidos en tu vida y tu cuerpo, te invito a experimentar este servicio.";
---

<Layout title={TITLE} description={DESCRIPTION}>
  <GeneralHero titles={["Armonización", "Energética Integral"]} />

  <SessionInfo />
  <Details />
</Layout>
